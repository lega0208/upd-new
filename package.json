{
  "name": "dua-upd",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx run-many --target=serve --projects=api,upd --parallel=2",
    "dev": "nx run-many --target=serve --projects=api,upd --parallel=2",
    "build": "nx run-many --target=build --projects=api,upd,db-updater,db-cli --parallel=false",
    "build:parallel": "nx run-many --target=build --projects=api,upd,db-updater,db-cli --parallel",
    "db-cli": "nx build db-cli && node dist/apps/db-cli/main.js",
    "db-updater": "npm run db-cli update",
    "db-repair": "npm run db-cli db-repair",
    "db-repopulate": "npm run db-cli repopulate",
    "deploy": "git pull && npm i && npm run build && cd docker && docker-compose build && docker-compose up -d",
    "lint": "nx run-many --target=lint --projects=api,upd,db-updater,db-cli --parallel",
    "test": "nx test",
    "analyze-bundle": "webpack-bundle-analyzer dist/apps/upd/stats.json",
    "pre-pr": "npm run lint && npm run build:parallel"
  },
  "private": true,
